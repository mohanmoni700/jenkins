pipeline {
parameters {
  string defaultValue: 'stage-1', name: 'stagename', trim: true
}


    agent any 
    stages {
        stage('stage-1') {
            when {
                stage == 'stage-1'
            }
            steps{
                sh '''
                echo "STEGE-1 executes if branch is master"
                '''
            }
        }

        stage('STAGE-2'){
            when {
                branch 'test'
            }
            steps{
                sh '''
                echo "STEGE-2 executes if branch is master"
                '''
            }
        }
    }
}
